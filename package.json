{
  "name": "the-last-fmylife",
  "version": "0.5.0",
  "description": "Lightweight app to retrieve (with a web crawler) and expose (with REST API) the last fmylife stories",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "crawler": "node crawler.js",
    "test": "mocha -R list test/*.js && jshint ."
  },
  "bin":{
    "mocha": "./node_modules/.bin/mocha",
    "jshint": "./node_modules/.bin/jshint"
  },
  "author": "Simon MAHE <mahe.simon@gmail.com>",
  "private": true,
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/5im0n/the-last-fmylife"
  },
  "dependencies": {
    "crawler": "~0.2.6",
    "underscore.string": "~2.3.3",
    "nconf": "~0.6.9",
    "mongoose": "~3.8.14",
    "moment": "~2.8.1",
    "restify": "~2.8.2",
    "underscore": "~1.6.0"
  },
  "devDependencies": {
    "jshint": "~2.5.3",
    "mocha": "~1.21.4"
  },
  "jshintConfig": {
    "strict": true,
    "unused": true,
    "indent": 4,
    "undef": true,
    "noempty": true,
    "freeze": true,
    "curly": true,
    "latedef": true,
    "maxcomplexity": 15,
    "trailing": true,
    "devel": true,
    "multistr": true,
    "node": true,
    "globals"   : {
      "describe"   : false,
      "it"         : false,
      "before"     : false,
      "beforeEach" : false,
      "after"      : false,
      "afterEach"  : false
    }
  }
}
